@model MvcTicariOtomasyon.Models.Sınıflar.Departman
@{
    ViewBag.Title = "departmanEkle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<br />
<h2>Yeni Departman Ekleme Sayfası</h2>
<br />

<form class="form-group" method="post">
    @Html.LabelFor(x => x.departmanAd)
    @Html.TextBoxFor(x => x.departmanAd, new { @class = "form form-control" })

    <br />
    <button class="btn btn-primary">Departman Ekle</button>
</form>

